<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:paddingLeft="16dp"
    android:paddingTop="16dp"
    android:paddingRight="16dp"
    android:paddingBottom="16dp"
    android:id="@+id/AdminFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".NavFragments.AdminFragment">


    <TextView
        android:id="@+id/textView"
        android:layout_width="170dp"
        android:layout_height="34dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="5dp"
        android:text="GestiÃ³n de plazas"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.087" />

    <LinearLayout
        android:id="@+id/LinearLayoutImagenCocheP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/rounded_rectangle"
        android:gravity="center_vertical">

        <ImageView
            android:id="@+id/imgCoche"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:contentDescription="Vector Asset"
            android:src="@drawable/baseline_directions_car_24" />
    </LinearLayout>


    <LinearLayout
        android:id="@+id/LinearLayoutImagenElectricoP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/LinearLayoutImagenCocheP"
        android:layout_alignParentStart="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="60dp"
        android:background="@drawable/rounded_rectangle"
        android:gravity="center_vertical">

        <ImageView
            android:id="@+id/imgElecP"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:contentDescription="Vector Asset"
            android:src="@drawable/baseline_electric_car_24" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LinearLayoutImagenMinusvP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/LinearLayoutImagenElectricoP"
        android:layout_alignParentStart="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="60dp"
        android:background="@drawable/rounded_rectangle"
        android:gravity="center_vertical">

        <ImageView
            android:id="@+id/imgMinusvP"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:contentDescription="Vector Asset"
            android:src="@drawable/baseline_accessible_24" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LinearLayoutImagenMotoP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/LinearLayoutImagenMinusvP"
        android:layout_alignParentStart="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="60dp"
        android:background="@drawable/rounded_rectangle"
        android:gravity="center_vertical">

        <ImageView
            android:id="@+id/imgMotoP"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:contentDescription="Vector Asset"
            android:src="@drawable/baseline_sports_motorsports_24" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layoutCarImg"
        android:layout_width="180dp"
        android:layout_height="72dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="70dp"
        android:layout_marginTop="45dp"
        android:layout_toEndOf="@+id/LinearLayoutImagenCocheP"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.397"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.411">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/btnRestaNormal"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_marginTop="10dp"
                android:clickable="true"
                android:contentDescription="Day_before_btton"
                android:src="@drawable/baseline_keyboard_arrow_left_24" />

            <TextView
                android:id="@+id/NumCocheNormal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginStart="8dp"
                android:layout_marginTop="6dp"
                android:layout_marginEnd="8dp"
                android:gravity="center"
                android:text="Num"
                android:textColor="@color/white"
                android:textSize="24sp" />

            <ImageView
                android:id="@+id/btonSumaNormal"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_gravity="center"
                android:layout_marginTop="10dp"
                android:background="?attr/selectableItemBackground"
                android:clickable="true"
                android:contentDescription="Next_day_btton"
                android:src="@drawable/baseline_keyboard_arrow_right_24" />

        </LinearLayout>

    </LinearLayout>

    <Button
        android:id="@+id/guardarButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/LinearLayoutImagenMotoP"
        android:layout_marginStart="5dp"
        android:layout_marginTop="50dp"
        android:layout_toEndOf="@+id/restablecerButton"
        android:text="Guardar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout" />
    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="184dp"
        android:layout_height="76dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="70dp"
        android:layout_marginTop="410dp"
        android:layout_marginBottom="144dp"
        android:layout_toEndOf="@+id/LinearLayoutImagenMotoP"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintVertical_bias="0.866">

        <ImageView
            android:id="@+id/btnRestaMoto"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginTop="10dp"
            android:clickable="true"
            android:contentDescription="Day_before_btton"
            android:src="@drawable/baseline_keyboard_arrow_left_24" />

        <TextView
            android:id="@+id/NumMoto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="8dp"
            android:layout_marginTop="6dp"
            android:layout_marginEnd="8dp"
            android:gravity="center"
            android:text="Num"
            android:textColor="@color/white"
            android:textSize="24sp" />

        <ImageView
            android:id="@+id/btonSumaMoto"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_gravity="center"
            android:layout_marginTop="10dp"
            android:background="?attr/selectableItemBackground"
            android:clickable="true"
            android:contentDescription="Next_day_btton"
            android:src="@drawable/baseline_keyboard_arrow_right_24" />

    </LinearLayout>

    <Button
        android:id="@+id/restablecerButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/LinearLayoutImagenMotoP"
        android:layout_alignParentStart="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="56dp"
        android:text="Restablecer"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/guardarButton"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        app:layout_constraintVertical_bias="0.0" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="70dp"
        android:layout_marginTop="295dp"
        android:layout_marginBottom="32dp"
        android:layout_toEndOf="@+id/LinearLayoutImagenMinusvP"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <ImageView
            android:id="@+id/btnRestaMinusv"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginTop="10dp"
            android:clickable="true"
            android:contentDescription="Day_before_btton"
            android:src="@drawable/baseline_keyboard_arrow_left_24" />

        <TextView
            android:id="@+id/NumMinusv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="8dp"
            android:layout_marginTop="6dp"
            android:layout_marginEnd="8dp"
            android:gravity="center"
            android:text="Num"
            android:textColor="@color/white"
            android:textSize="24sp" />

        <ImageView
            android:id="@+id/btonSumaMinusv"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_gravity="center"
            android:layout_marginTop="10dp"
            android:background="?attr/selectableItemBackground"
            android:clickable="true"
            android:contentDescription="Next_day_btton"
            android:src="@drawable/baseline_keyboard_arrow_right_24" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/layoutCarImg"
        android:layout_marginStart="70dp"
        android:layout_marginTop="45dp"
        android:layout_marginBottom="388dp"
        android:layout_toEndOf="@+id/LinearLayoutImagenElectricoP"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/layoutCarImg"
        app:layout_constraintVertical_bias="0.0">

        <ImageView
            android:id="@+id/btnRestaCocheElec"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_marginTop="10dp"
            android:clickable="true"
            android:contentDescription="Day_before_btton"
            android:src="@drawable/baseline_keyboard_arrow_left_24" />

        <TextView
            android:id="@+id/NumCocheElec"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="8dp"
            android:layout_marginTop="6dp"
            android:layout_marginEnd="8dp"
            android:gravity="center"
            android:text="Num"
            android:textColor="@color/white"
            android:textSize="24sp" />

        <ImageView
            android:id="@+id/btonSumaElec"
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:layout_gravity="center"
            android:layout_marginTop="10dp"
            android:background="?attr/selectableItemBackground"
            android:clickable="true"
            android:contentDescription="Next_day_btton"
            android:src="@drawable/baseline_keyboard_arrow_right_24" />

    </LinearLayout>

</RelativeLayout>