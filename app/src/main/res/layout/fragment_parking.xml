<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:paddingLeft="16dp"
    android:paddingTop="16dp"
    android:paddingRight="16dp"
    android:paddingBottom="16dp"
    android:id="@+id/ParkingFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".NavFragments.ParkingFragment">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ProgressBar
            android:id="@+id/progressBarP"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:visibility="gone" />
    </FrameLayout>

    <TextView
        android:id="@+id/fechaTextViewP"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="14dp"
        android:gravity="center"
        android:text="Fecha actual"
        android:textColor="@color/white"
        android:textSize="24sp" />

    <ImageView
        android:id="@+id/btnAnteriorP"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="10dp"
        android:layout_alignParentStart="true"
        android:clickable="true"
        android:contentDescription="Day_before_btton"
        android:src="@drawable/baseline_keyboard_arrow_left_24" />

    <ImageView
        android:id="@+id/btnSiguienteP"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="10dp"
        android:layout_alignParentEnd="true"
        android:src="@drawable/baseline_keyboard_arrow_right_24"
        android:clickable="true"
        android:background="?attr/selectableItemBackground"
        android:contentDescription="Next_day_btton" />

    <LinearLayout
        android:id="@+id/LinearLayoutImagenCocheP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/fechaTextViewP"
        android:layout_alignParentStart="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="80dp"
        android:background="@drawable/rounded_rectangle"
        android:gravity="center_vertical">

        <ImageView
            android:id="@+id/imgCoche"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:contentDescription="Vector Asset"
            android:src="@drawable/baseline_directions_car_24" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LLayoutN"
        android:layout_width="259dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/LinearLayoutImagenCocheP"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="-70dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="438dp"
        android:layout_toEndOf="@+id/LinearLayoutImagenElectricoP"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerViewReservasN"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/LinearLayoutImagenElectricoP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/LinearLayoutImagenCocheP"
        android:layout_alignParentStart="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="70dp"
        android:background="@drawable/rounded_rectangle"
        android:gravity="center_vertical">

        <ImageView
            android:id="@+id/imgElecP"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:contentDescription="Vector Asset"
            android:src="@drawable/baseline_electric_car_24" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LLayoutE"
        android:layout_width="259dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/LinearLayoutImagenCocheP"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="285dp"
        android:layout_toEndOf="@+id/LinearLayoutImagenElectricoP"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerViewReservasE"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/LinearLayoutImagenMinusvP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/LinearLayoutImagenElectricoP"
        android:layout_alignParentStart="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="70dp"
        android:background="@drawable/rounded_rectangle"
        android:gravity="center_vertical">

        <ImageView
            android:id="@+id/imgMinusvP"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:contentDescription="Vector Asset"
            android:src="@drawable/baseline_accessible_24" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LLayoutMinusv"
        android:layout_width="259dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/LinearLayoutImagenElectricoP"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="160dp"
        android:layout_toEndOf="@+id/LinearLayoutImagenMotoP"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerViewReservasMinusv"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LinearLayoutImagenMotoP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/LinearLayoutImagenMinusvP"
        android:layout_alignParentStart="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="70dp"
        android:background="@drawable/rounded_rectangle"
        android:gravity="center_vertical">

        <ImageView
            android:id="@+id/imgMotoP"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:contentDescription="Vector Asset"
            android:src="@drawable/baseline_sports_motorsports_24" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LLayoutM"
        android:layout_width="259dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/LinearLayoutImagenElectricoP"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="210dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="6dp"
        android:layout_toEndOf="@+id/LinearLayoutImagenMotoP"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerViewReservasM"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </LinearLayout>

</RelativeLayout>